[
    {
        "description": "unevaluatedProperties with $ref",
        "schema": {
            "$schema": "https://json-schema.org/draft/2019-09/schema",
            "type": "object",
            "$ref": "#/$defs/bar",
            "properties": {
                "foo": { "type": "string" }
            },
            "unevaluatedProperties": false,
            "$defs": {
                "bar": {
                    "properties": {
                        "bar": { "type": "string" }
                    }
                }
            }
        },
        "tests": [
            {
                "description": "with no unevaluated properties",
                "data": {
                    "foo": "foo",
                    "bar": "bar"
                },
                "valid": true
            }/*,
            {
                "description": "with unevaluated properties",
                "data": {
                    "foo": "foo",
                    "bar": "bar",
                    "baz": "baz"
                },
                "valid": false
            }*/
        ]
    }
]
